{% extends 'base.html.twig' %}
{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block body %}
    {{ parent() }}
    {% block form %}
    <h1> User Sign Up:</h1>
        {{ form_start(form) }}
        {{ form_row(form.userName) }}
        {{ form_row(form.password) }}
        {{ form_row(form.email) }}
        {{ form_row(form.gender) }}
        <button type="submit" class="btn btn-primary">Save</button>
        {{ form_end(form) }}
    {% endblock %}
{% endblock  %}